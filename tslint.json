{
  "extends": "tslint:standard",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "quotemark": false,
    "no-console": false,
    "trailing-comma": false,
    "semicolon": false,
    "no-unused-expression": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
